<%- include('../partials/header') %>
<table id="list">
    <thead>
      <tr>
        <th>Airline</th>
        <th>Airport</th>
        <th>Flight No.</th>
        <th>Trips </th>
        <th>Edit Flight</th>
      </tr>
    </thead>
    <tbody>
      <% flights.forEach( (f) => { %>
        <tr>
          <td><%= f.airline %></td>
          <td><%= f.airport %></td>
          <td><%= f.flightNo %></td>
          <td><%= f.departs %></td>
          <td><a id="flightdetails" href="/flights/<%= f._id %>">Flight Details</a></td>
        </tr>
      <% }); %>
    </tbody>
  </table>

<%- include('../partials/footer') %>